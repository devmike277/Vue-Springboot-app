  this.tasks = response.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        })\n        .finally(() => {\n          //Do somethin if it's needed\n        });\n    },\n    deleteTask(id) {\n      TaskService.deleteTask(id)\n        .then((response) => {\n          console.log(response.data);\n          if(response.data.deleted){\n            window.location.href = 'list-tasks';\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        })\n        .finally(() => {\n          //Do somethin if it's needed\n        });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}